<aEBsystem:AEBSystem
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:aEBsystem="http://www.example.org/aEBsystem"
    xsi:schemaLocation="http://www.example.org/aEBsystem aEBsystem.ecore"
    systemID="AEB006"
    operatingMode="Emergency">
  <sensors sensorType="Radar"
      range="15.0"
      accuracy="92.0">
    <detectionEvents
        timestamp="1698765480"
        distance="12.0"
        confidence="85.0">
      <obstacle
          obstacleType="Vehicle"
          speed="40.0"
          size="4.5"/>
    </detectionEvents>
  </sensors>
  <controller
      state="Acting"
      decisionLatency="0.2">
    <actions
        actionType="Steer"
        responseTime="0.8"
        priority="1"
        executedBy="//@actuators.1"/>
    <actions
        actionType="Warn"
        responseTime="0.5"
        priority="2"
        executedBy="//@actuators.2"/>
    <constraints
        constraintType="Timing"
        threshold="0.5"
        isCritical="true"
        appliesTo="//@controller/@actions.0 //@controller/@actions.1"/>
  </controller>
  <actuators
      type="Brake"
      maxForce="1000.0"
      status="false"/>
  <actuators
      type="Steering"
      maxForce="600.0"
      status="true"
      actions="//@controller/@actions.0"/>
  <actuators
      type="Horn"
      maxForce="0.0"
      status="true"
      actions="//@controller/@actions.1"/>
</aEBsystem:AEBSystem>