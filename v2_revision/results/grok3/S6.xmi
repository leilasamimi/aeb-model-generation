<aEBsystem:AEBSystem
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:aEBsystem="http://www.example.org/aEBsystem"
    xsi:schemaLocation="http://www.example.org/aEBsystem aEBsystem.ecore"
    systemID="AEB006"
    operatingMode="Emergency">
  <sensors sensorType="Radar"
      range="8.0"
      accuracy="92.0">
    <detectionEvents
        timestamp="1698765480"
        distance="4.0"
        confidence="85.0">
      <obstacle
          obstacleType="Vehicle"
          speed="25.0"
          size="3.5"/>
    </detectionEvents>
  </sensors>
  <controller
      state="Acting"
      decisionLatency="0.05">
    <actions
        actionType="Brake"
        responseTime="0.3"
        priority="1"
        executedBy="//@actuators.0"/>
    <constraints
        constraintType="Timing"
        threshold="0.3"
        isCritical="true"
        appliesTo="//@controller/@actions.0"/>
  </controller>
  <actuators
      type="Brake"
      maxForce="1300.0"
      status="true"
      actions="//@controller/@actions.0"/>
</aEBsystem:AEBSystem>