<aEBsystem:AEBSystem
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:aEBsystem="http://www.example.org/aEBsystem"
    xsi:schemaLocation="http://www.example.org/aEBsystem aEBsystem.ecore"
    systemID="AEB006"
    operatingMode="Emergency">
  <sensors sensorType="Radar"
      range="12.0"
      accuracy="92.0">
    <detectionEvents
        timestamp="1698765480"
        distance="9.5"
        confidence="85.0">
      <obstacle
          obstacleType="Vehicle"
          speed="25.0"
          size="4.5"/>
    </detectionEvents>
  </sensors>
  <sensors sensorType="Lidar"
      range="10.0"
      accuracy="90.0">
    <detectionEvents
        timestamp="1698765481"
        distance="6.0"
        confidence="88.0">
      <obstacle
          obstacleType="Pedestrian"
          speed="5.0"
          size="1.7"/>
    </detectionEvents>
  </sensors>
  <controller
      state="Acting"
      decisionLatency="0.1">
    <actions
        actionType="Brake"
        responseTime="0.4"
        priority="1"
        executedBy="//@actuators.0"/>
    <constraints
        constraintType="Timing"
        threshold="0.4"
        isCritical="true"
        appliesTo="//@controller/@actions.0"/>
  </controller>
  <actuators
      type="Brake"
      maxForce="1300.0"
      status="true"
      actions="//@controller/@actions.0"/>
  <environmentconditions
      conditionType="Rain"
      severity="50.0"
      affects="//@sensors.0 //@sensors.1"/>
</aEBsystem:AEBSystem>