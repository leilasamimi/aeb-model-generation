<aEBsystem:AEBSystem
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:aEBsystem="http://www.example.org/aEBsystem"
    xsi:schemaLocation="http://www.example.org/aEBsystem aEBsystem.ecore"
    systemID="AEB100"
    operatingMode="Emergency">
  <sensors sensorType="Radar"
      range="10.0"
      accuracy="94.0">
    <detectionEvents
        timestamp="1699000000"
        distance="9.2"
        confidence="93.0">
      <obstacle
          obstacleType="Vehicle"
          speed="25.0"
          size="4.2"/>
    </detectionEvents>
  </sensors>
  <controller
      state="Acting"
      decisionLatency="0.12">
    <actions
        actionType="Brake"
        responseTime="0.5"
        priority="1"
        executedBy="//@actuators.0"/>
    <constraints
        constraintType="Timing"
        threshold="0.5"
        isCritical="true"
        appliesTo="//@controller/@actions.0"/>
  </controller>
  <actuators
      type="Brake"
      maxForce="1050.0"
      status="true"
      actions="//@controller/@actions.0"/>
</aEBsystem:AEBSystem>