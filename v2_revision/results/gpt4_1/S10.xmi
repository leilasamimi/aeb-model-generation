<aEBsystem:AEBSystem
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:aEBsystem="http://www.example.org/aEBsystem"
    xsi:schemaLocation="http://www.example.org/aEBsystem aEBsystem.ecore"
    systemID="AEB006"
    operatingMode="Normal">
  <sensors sensorType="Radar"
      range="12.0"
      accuracy="80.0">
    <detectionEvents
        timestamp="1698765480"
        distance="7.5"
        confidence="65.0">
      <obstacle
          obstacleType="Vehicle"
          speed="24.0"
          size="4.0"/>
    </detectionEvents>
  </sensors>
  <controller
      state="Assessing"
      decisionLatency="0.5">
    <actions
        actionType="Brake"
        responseTime="1.0"
        priority="2"
        executedBy="//@actuators.0"/>
    <constraints
        constraintType="Timing"
        threshold="1.0"
        isCritical="false"
        appliesTo="//@controller/@actions.0"/>
  </controller>
  <actuators
      type="Brake"
      maxForce="800.0"
      status="true"
      actions="//@controller/@actions.0"/>
  <environmentconditions
      conditionType="Fog"
      severity="75.0"
      affects="//@sensors.0"/>
</aEBsystem:AEBSystem>