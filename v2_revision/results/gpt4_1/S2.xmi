<aEBsystem:AEBSystem
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:aEBsystem="http://www.example.org/aEBsystem"
    xsi:schemaLocation="http://www.example.org/aEBsystem aEBsystem.ecore"
    systemID="AEB006">
  <sensors sensorType="Radar"
      range="60.0"
      accuracy="93.0">
    <detectionEvents
        timestamp="1698765500"
        distance="25.0"
        confidence="92.0">
      <obstacle
          obstacleType="Vehicle"
          speed="55.0"
          size="4.5"/>
    </detectionEvents>
  </sensors>
  <controller
      state="Acting"
      decisionLatency="0.2">
    <actions
        actionType="Warn"
        responseTime="1.0"
        priority="1"
        executedBy="//@actuators.0"/>
    <constraints
        constraintType="Timing"
        threshold="1.0"
        isCritical="true"
        appliesTo="//@controller/@actions.0"/>
  </controller>
  <actuators
      type="Horn"
      status="true"
      actions="//@controller/@actions.0"/>
</aEBsystem:AEBSystem>