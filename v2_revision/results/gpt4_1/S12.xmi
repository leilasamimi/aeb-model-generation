<aEBsystem:AEBSystem
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:aEBsystem="http://www.example.org/aEBsystem"
    xsi:schemaLocation="http://www.example.org/aEBsystem aEBsystem.ecore"
    systemID="AEB006"
    operatingMode="Emergency">
  <sensors sensorType="Camera"
      range="10.0"
      accuracy="93.0">
    <detectionEvents
        timestamp="1698765480"
        distance="5.0"
        confidence="90.0">
      <obstacle
          obstacleType="Pedestrian"
          speed="30.0"
          size="1.8"/>
    </detectionEvents>
  </sensors>
  <controller
      state="Acting"
      decisionLatency="0.07">
    <actions
        actionType="Brake"
        responseTime="0.4"
        priority="1"
        executedBy="//@actuators.0"/>
    <actions
        actionType="Warn"
        responseTime="0.4"
        priority="2"
        executedBy="//@actuators.1"/>
    <constraints
        constraintType="Timing"
        threshold="0.4"
        isCritical="true"
        appliesTo="//@controller/@actions.0 //@controller/@actions.1"/>
    <constraints
        constraintType="Distance"
        threshold="5.0"
        isCritical="false"
        appliesTo="//@controller/@actions.0 //@controller/@actions.1"/>
  </controller>
  <actuators
      type="Brake"
      maxForce="1100.0"
      status="true"
      actions="//@controller/@actions.0"/>
  <actuators
      type="Horn"
      maxForce="0.0"
      status="true"
      actions="//@controller/@actions.1"/>
</aEBsystem:AEBSystem>