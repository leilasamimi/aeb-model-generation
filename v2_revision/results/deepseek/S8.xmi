<aEBsystem:AEBSystem
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:aEBsystem="http://www.example.org/aEBsystem"
    xsi:schemaLocation="http://www.example.org/aEBsystem aEBsystem.ecore"
    systemID="AEB006"
    operatingMode="Emergency">
  <sensors sensorType="Radar"
      range="5.0"
      accuracy="98.0">
    <detectionEvents
        timestamp="1698765480"
        distance="2.5"
        confidence="95.0">
      <obstacle
          obstacleType="Vehicle"
          speed="40.0"
          size="4.5"/>
    </detectionEvents>
  </sensors>
  <controller
      state="Acting"
      decisionLatency="0.02">
    <actions
        responseTime="0.2"
        priority="1"
        executedBy="//@actuators.0"/>
    <constraints
        constraintType="Timing"
        threshold="0.2"
        isCritical="true"
        appliesTo="//@controller/@actions.0"/>
    <constraints
        constraintType="Distance"
        threshold="3.0"
        isCritical="true"
        appliesTo="//@controller/@actions.0"/>
  </controller>
  <actuators
      maxForce="1500.0"
      status="true"
      actions="//@controller/@actions.0"/>
</aEBsystem:AEBSystem>